{
  "name": "@nexscript/utils",
  "version": "1.0.2",
  "description": "NexScript utilities and types",
  "keywords": [
    "nexa",
    "nexscript",
    "sdk",
    "smart contracts"
  ],
  "homepage": "https://nexscript.org",
  "bugs": {
    "url": "https://gitlab.com/nexa/otoplo/nexscript/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/nexa/otoplo/nexscript.git"
  },
  "license": "MIT",
  "author": "Rosco Kalis <roscokalis@gmail.com>",
  "contributors": [
    "Gabriel Cardona <gabriel@bitcoin.com>",
    "Dolaned <dolaned@nexa.org>",
    "Nexa Js devs <js-devs@nexa.org>"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:test": "yarn clean:test && yarn compile:test",
    "clean": "rm -rf ./dist",
    "clean:test": "rm -rf ./dist-test",
    "compile": "tsc -p tsconfig.build.json",
    "compile:test": "tsc -p tsconfig.test.json",
    "lint": "eslint . --ext .ts --ignore-path ../../.eslintignore",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "pretest": "yarn build:test",
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest"
  },
  "dependencies": {
    "@bitauth/libauth": "^2.1.0",
    "hash.js": "^1.1.7",
    "merkletreejs": "^0.3.11"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.1",
    "eslint": "^7.20.0",
    "jest": "^29.4.1",
    "typescript": "^4.1.5"
  },
  "gitHead": "d45653fba1217e550e829843932bb56ef5f3a40c"
}
