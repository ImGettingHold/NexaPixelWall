{
  "name": "@nexscript/nexscript",
  "version": "1.0.2",
  "description": "Easily write and interact with Nexa contracts",
  "keywords": [
    "nexa",
    "nexscript",
    "sdk",
    "smart contracts"
  ],
  "homepage": "https://nexscript.org",
  "bugs": {
    "url": "https://gitlab.com/nexa/otoplo/nexscript/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/nexa/otoplo/nexscript.git"
  },
  "license": "MIT",
  "author": "Rosco Kalis <roscokalis@gmail.com>",
  "contributors": [
    "Gabriel Cardona <gabriel@bitcoin.com>",
    "Dolaned <dolaned@nexa.org>",
    "Nexa Js devs <js-devs@nexa.org>"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:test": "yarn clean:test && yarn compile:test",
    "clean": "rm -rf ./dist",
    "clean:test": "rm -rf ./dist-test",
    "compile": "tsc -p tsconfig.build.json",
    "compile:test": "tsc -p tsconfig.test.json",
    "lint": "eslint . --ext .ts --ignore-path ../../.eslintignore",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test",
    "pretest": "yarn build:test",
    "test": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --runInBand"
  },
  "dependencies": {
    "@bitauth/libauth": "2.1.0",
    "@nexscript/utils": "^1.0.2",
    "@vgrunner/electrum-cash": "^2.0.12",
    "bip68": "^1.0.4",
    "bitcoin-rpc-promise-retry": "^1.3.0",
    "delay": "^5.0.0",
    "fast-deep-equal": "^3.1.3",
    "nexcore-lib": "^2.4.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.1",
    "@psf/bch-js": "^4.15.0",
    "bip39": "^3.0.4",
    "eslint": "^7.20.0",
    "jest": "^29.4.1",
    "typescript": "^4.1.5"
  },
  "gitHead": "d45653fba1217e550e829843932bb56ef5f3a40c"
}
